[constant]

# Scavenging is applied above the following threshold over rain [mm/h].
Scavenging_rain_threshold: 1.

# The following constant values are given to the
# below-cloud scavenging coefficients of gases.

# species        scavenging_constant (1/s)
all                  7.e-4
I2	





[constant_aer]

# The following constant values are given to the
# below-cloud scavenging coefficients of PMs.

# bin index       scavenging_constant_aer (1/s)
