#--------------------------------------------------------------------
[pudykiewicz]

# In-cloud scavenging according to Pudykiewicz is applied above the 
# following threshold value for the relative humidity.
Pudykiewicz_threshold: 0.8

# The following values are given to the constant
# in the Pudykiewicz formula for in-cloud scavenging.
# This option is made available for gases, as well as, PMs.

# species       Pudykiewicz_constant (1/s)
    all	     		3.5e-5
    I2	             	3.5e-5





[pudykiewicz_aer]

Pudykiewicz_aer_threshold: 0.8

# bin index       Pudykiewicz_constant_aer (1/s)
    all	     		3.5e-5
     0	             	3.5e-5	
     1	             	3.5e-5	
     2	             	3.5e-5	
     3	             	3.5e-5	
     4	             	3.5e-5	

